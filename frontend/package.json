{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "i18n:generate": "i18next-resources-for-ts interface -i ./public/locales/en -o ./src/i18n/resources.d.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "tidy": "npm run format && npm run lint",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui --api 9527",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.16.6",
    "@mui/material": "^5.16.6",
    "@mui/x-date-pickers": "^7.12.0",
    "@tanstack/react-query": "^4.24.10",
    "@tanstack/react-query-devtools": "^4.24.12",
    "axios": "^1.7.3",
    "date-fns": "^3.6.0",
    "deep-object-diff": "^1.1.9",
    "dompurify": "^3.1.6",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "is-base64": "^1.1.0",
    "material-react-table": "^2.13.1",
    "material-ui-popup-state": "^5.1.2",
    "oidc-client-ts": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.34.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^15.0.1",
    "react-router-dom": "^6.3.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.27.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/dompurify": "^3.0.5",
    "@types/is-base64": "^1.1.3",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^2.0.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-i18next": "^6.0.9",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-hooks-addons": "^0.3.1",
    "eslint-plugin-testing-library": "^6.2.2",
    "i18next-resources-for-ts": "^1.5.0",
    "jsdom": "^20.0.1",
    "typescript": "^5.0.4",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.5"
  }
}
