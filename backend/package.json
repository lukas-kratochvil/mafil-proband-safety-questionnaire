{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
    "tidy": "npm run format && npm run lint",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:migrate:dev": "prisma migrate deploy && prisma generate && npm run start:dev",
    "start:debug": "cross-env NODE_ENV=development nest start --debug",
    "start:prod": "node dist/src/main",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "seed": "node dist/src/prisma/seeding/seed",
    "seed:local": "ts-node src/prisma/seeding/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/common": "^9.3.12",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^9.3.12",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/throttler": "^6.1.1",
    "@prisma/client": "^5.18.0",
    "base64-stream": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
    "graphql": "^16.9.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "nest-winston": "^1.9.7",
    "pdfkit": "^0.14.0",
    "prisma": "^5.18.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "token-introspection": "^3.3.0",
    "winston": "^3.14.1"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^5.0.18",
    "@nestjs/cli": "^10.4.4",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^9.3.12",
    "@types/base64-stream": "^1.0.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.3.0",
    "@types/pdfkit": "^0.13.4",
    "@types/supertest": "^6.0.2",
    "@types/token-introspection": "^3.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
}
