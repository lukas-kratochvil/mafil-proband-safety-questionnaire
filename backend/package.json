{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:migrate:dev": "prisma migrate deploy && prisma generate && cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug",
    "start:prod": "node dist/main",
    "start:migrate:prod": "prisma migrate deploy && node dist/main",
    "seed": "node dist/prisma/seeding/seed",
    "seed:local": "ts-node src/prisma/seeding/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@nestjs/apollo": "^11.0.4",
    "@nestjs/common": "^9.3.12",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.3.12",
    "@nestjs/graphql": "^11.0.4",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/throttler": "^4.0.0",
    "@prisma/client": "^5.6.0",
    "base64-stream": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "helmet": "^6.0.1",
    "joi": "^17.11.0",
    "nest-winston": "^1.9.1",
    "pdfkit": "^0.13.0",
    "prisma": "^5.6.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^3.16.0",
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^9.3.12",
    "@types/base64-stream": "^1.0.2",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.4.0",
    "@types/node": "^16.18.14",
    "@types/pdfkit": "^0.12.9",
    "@types/supertest": "^2.0.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "jest": "^29.4.3",
    "jest-mock-extended": "^3.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.9.5"
  }
}
